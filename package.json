{
  "name": "hashiiiii-homepage",
  "version": "0.1.0",
  "description": "Personal homepage for hashiiiii.com",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsx src/lib/build-blog.ts && vite build",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "format": "biome check --write .",
    "prepare": "husky"
  },
  "dependencies": {
    "gray-matter": "^4.0.3",
    "highlight.js": "^11.11.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.2",
    "rss-parser": "^3.13.0",
    "zenn-content-css": "^0.2.10",
    "zenn-embed-elements": "^0.2.10",
    "zenn-markdown-html": "^0.2.10"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20.19.1",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.5.2",
    "@vitest/coverage-v8": "^1.6.1",
    "autoprefixer": "^10.4.21",
    "husky": "^9.0.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.16",
    "tsx": "^4.20.3",
    "typescript": "^5.0.0",
    "unimported": "^1.31.0",
    "vite": "^6.3.5",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
